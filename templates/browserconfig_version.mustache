{{!
    @template local_softwarewarning/browserconfigmultirule

    Example context (json):
    {
      "severity": "error",
      "text": "Your browser is not supported!",
      "closable": false
    }
}}
<div class="py-2">
    Below version (inclusive)
    <input class="form-control mx-2" size="10" type="number" id="sw-{{i}}-rule{{j}}-version" value="{{version}}">
    set Banner
    <select class="custom-select mx-2" id="sw-{{i}}-rule{{j}}-banner" {{#banner}}data-selected="{{banner}}"{{/banner}}>
    {{# banners }}
        <option value="{{.}}">{{ . }}</option>
    {{/ banners }}
    </select>
</div>